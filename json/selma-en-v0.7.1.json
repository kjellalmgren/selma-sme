        "status" : "[Error Code]",
        "error" : "[Error Text]",
        "message" : "[Error Reason]"
      }
    },
    "caseIdStatus" : {
      "type" : "string",
      "description" : "Set status on the caseId",
      "example" : "CLOSEDBYAPPLICANT",
      "enum" : [ "CLOSEDBYAPPLICANT", "STARTEDBYAPPLICANT", "CLOSEDBYOFFICER", "CLOSEDBYTHINNING", "READYFOROFFICER" ]
    },
    "processId" : {
      "type" : "string",
